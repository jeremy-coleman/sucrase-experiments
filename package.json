{
  "name": "electron-sucrase-starter",
  "version": "0.0.1",
  "description": "Electron sucrase",
  "main": "src/desktop/main.ts",
  "bin": {
    "sucrase-serve": "./cli.js"
  },
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild",
    "pkg": "gulp build",
    "peek-tasks": "gulp --tasks-simple",
    "ssr:start": "sucrase-node ./src/ssr/cli.ts src/docs/App.tsx --bundle",
    "ssr:async": "sucrase-node ./src/ssr/cli.ts src/docs/Async",
    "ssr:styled": "sucrase-node ./src/ssr/cli.ts src/docs/StyledComponents",
    "ssr:bundle": "sucrase-node ./src/ssr/cli.ts src/docs/Bundle.tsx --bundle",
    "ssr:icon": "sucrase-node ./src/ssr/cli.ts src/docs/Icon.js -n"
  },
  "author": "jeremy",
  "license": "MIT",
  "dependencies": {
    "browserify": "^16.2.3",
    "clsx": "1.0.3",
    "dotenv": "^6.2.0",
    "electron-rebuild": "1.8.4",
    "find-up": "3.0.0",
    "gulp": "4",
    "json-stringify-safe": "^5.0.1",
    "meow": "^4.0.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "redux": "4.0.1",
    "styled-components": "4.1.3",
    "styled-system": "4.0.1",
    "sucrase": "^3.9.6",
    "terser": "^3.16.1",
    "tinyify": "^2.5.0",
    "update-notifier": "^2.5.0",
    "watchify": "^3.11.1"
  },
  "devDependencies": {
    "@types/node": "11.9.5",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "cxs": "6.2.0",
    "electron": "4.0.7",
    "node-fetch": "2.3.0",
    "tslint": "5.13.1",
    "typescript": "^3.3.3333",
    "typescript-tslint-plugin": "0.3.1"
  }
}
